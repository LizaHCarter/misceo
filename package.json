    "grunt-deploy": "^0.1.7",
    "grunt-hapi": "^0.8.1",
    "grunt-jscs": "^0.8.1",
    "grunt-shell": "^1.1.1",
    "hapi": "^7.5.2",
    "hapi-auth-basic": "^1.1.1",
    "hapi-auth-cookie": "^1.4.1",
    "joi": "^4.9.0",
    "jshint-stylish": "^1.0.0",
