  },
  "homepage": "https://github.com/lizahcarter/misceo",
  "dependencies": {
    "async": "^0.9.0",
    "bcrypt": "^0.8.0",
    "bower": "^1.3.12",
    "crawler": "^0.4.1",
    "good": "^4.0.0",
    "good-console": "^2.0.1",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-copy": "^0.7.0",
    "grunt-contrib-jade": "^0.13.0",
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-contrib-less": "^0.12.0",
