    "node": "0.10.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lizahcarter/misceo"
  },
  "author": "Adam Barnhard, Mikey Badr, Liza Carter, Melanie Fryman, Logan Richardson, Nathan Vasquez",
  "license": "MIT",
  "bugs": {
