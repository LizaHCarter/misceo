#contact-form-modal.modal(data-featurette='help-modal', style='; display: none;')
  .grid-container
    .grid-60.centered
      #help-form.contained.featurette(data-featurette='help-form')
        .grid-100.secondary-heading
          h2 Contact Support
          button.modal-close.small.square.secondary(data-modal-close='close')
        .grid-70
          .message.waiting
            span.loading-spinner.loading-spinner-large
              span
          .message.success
            p
              strong Thanks!
              | We'll be in touch.
          form#contact-form.new_contact_form(accept-charset='UTF-8', action='/contact', method='post', name='contact-form')
            div(style='display:none')
              input(name='utf8', type='hidden', value='✓')
              input(name='authenticity_token', type='hidden', value='vKkaN9eFUTmj+pLVYy1ekMpv4baGBBzwbZ36zMA7rsU=')
