#container(data-layout-element='container')
  #content
    .grid-container
      .grid-100.tour-parent.workspace-welcome
        .contained
          .grid-100
            .tour-callout
              .welcome-panel(data-featurette='workspaces-tour')
                a.close-tour.button.secondary.button-close(href='#')
                .grid-100
                  h2 Getting started with Workspaces
                  p.subheader
                    | A fully browser-based code editor
                    | designed to make learning with Treehouse a breeze.
                .grid-100.grid-parent
                  .grid-50.track-callout
                    h4 Using Workspaces
                    p
                      | If you've used a native text editor to code, you'll
                      | find that Workspaces isn't much different. Work alongside
                      | a Treehouse course or build something completely from
                      | scratch. Ready to learn how it works?
                    a.button(data-tour-trigger-start='', href='#')
                      | Start
                      | tour
                  .grid-50
                    h4 Workspace Storage
                    p
                      | Each Workspace has a total storage limit of 50 MB.
                      | Currently, students are allowed to have up to 100
                      | Workspaces created at a time.
      // Workspaces Controls
      .grid-100(data-featurette='workspaces-index')
        // Page Controls
        .workspace-controls.contained.control-container.control-tour(data-layout-element='control-bar')
          // Search
          .search-container
            form.control-search.search-form
              input#q(autocomplete='off', name='q', placeholder='Search...', type='text')
              label.search-open(for='q')
          // Control Page Items
          ul.control-page-items
            // Sorting
            li.dropdown-parent
              a.dropdown-parent-label
                span.currently-selected Latest
              ul#sort.dropdown-child.filter-dropdown(data-featurette='filter-list')
                li
                  a.selected(data-filter-value='') Latest
                li
                  a(data-filter-value='title') Name
            // Filtering
            li.dropdown-parent.filter-container
              // Env
              a.dropdown-parent-label
                span.currently-selected All Environments
              ul#env.dropdown-child.filter-dropdown(data-featurette='filter-list')
                li
                  a.selected(data-filter-value='')
                    | All
                    | Environments
                li
                  a(data-filter-value='1') Static
                li
                  a(data-filter-value='2') Python
                li
                  a(data-filter-value='3') Ruby
                li
                  a(data-filter-value='4') PHP5
                li
                  a(data-filter-value='5') Sass
                li
                  a(data-filter-value='7') Java
            li.dropdown-parent.filter-container
              // Courses
              a.dropdown-parent-label
                span.currently-selected All Courses
              ul#course.dropdown-child.filter-dropdown(data-featurette='filter-list')
                li
                  a.selected(data-filter-value='') All Courses
      // Workspaces Table Layout
      .grid-100
        .workspace-table.contained
          .top
            .grid-100
              a.new-workspace(data-featurette='modal-trigger', data-mobile-fallback='doit', data-target='new-workspace-modal', href='/workspaces/new')
                | New
                span.icon.icon-add-green
              .workspace-capacity
                p
                  | 0 of
                  span 100
                .capacity-container.progress-container
                  .capacity-status.progress-status(style='width: 0.0%')
          #workspaces-pjax
            ul.workspace-list
              li.workspace-item.workspace-tour-step-placeholder.workspace-example
                .workspace-item-object.workspace-meta.grid-40
                  h3 Example Workspace
                  p Started from Blank Template
                .workspace-item-object.workspace-env.grid-10
                  p.env
                    strong Static
                .workspace-item-object.workspace-actions.grid-20.right
                  a.workspace-fork(href='')
                    span.icon.icon-fork
                  a.workspace-settings(href='')
                    span.icon.icon-settings
              li.workspace-item.empty
                #no-results.empty-message
                  p You have no Workspaces
